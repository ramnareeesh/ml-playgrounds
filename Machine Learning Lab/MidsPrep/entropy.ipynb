   "source": [
    "def entropy(a, b):\n",
    "    from math import log2\n",
    "    pa = a/(a+b)\n",
    "    pb = b/(a+b)\n",
    "    return \"{:.3f}\".format((-pa * log2(pa)) + (-pb * log2(pb)))"
   ]
